{
  "baseUrl": "http://localhost:6006",
  "supportFile": "cypress/support.js",
  "pluginsFile": "cypress/plugins.js",
  "integrationFolder": "src",
  "testFiles": "**/*.test.js",
  "ignoreTestFiles": [
    "**/__snapshots__/*",
    "**/__image_snapshots__/*"
  ],
  "video": false,
  "env": {
    "cypress-plugin-snapshots": {
      "autoCleanUp": true,
      "imageConfig": {
        "threshold": 0.05
      }
    }
  }
}
